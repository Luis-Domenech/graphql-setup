{
  "name": "graphql-setup",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch": "tsc -w",
    "clean": "rm -rf ./dist",
    "build": "yarn clean && yarn gen && tsc",
    "gen-env": "gen-env-types .env -o src/env.d.ts -e .",
    "reup": "yarn build && node dist/index.js",
    "start": "node dist/index.js",
    "reset-db-old": "node psqlSetup.js && prisma migrate save --name 'Initial DB Setup' --experimental && prisma migrate up --experimental && prisma generate",
    "db-init": "node scripts/psqlReset.js && prisma migrate dev --name 'Initial DB Setup' && prisma migrate deploy && prisma generate",
    "connect": "sh scripts/connectToServer.sh",
    "deploy-ver": "yarn docker-wipe && yarn build && chmod +x scripts/deployVersion.sh && scripts/deployVersion.sh",
    "deploy": "yarn docker-wipe && yarn build && chmod +x scripts/deployLatest.sh && scripts/deployLatest.sh",
    "docker-wipe": "sudo docker system prune -a --volumes",
    "introspect": "prisma introspect",
    "gen": "prisma generate",
    "gen-w": "prisma generate --watch",
    "create:migration": "prisma migrate dev",
    "realize:migration": "prisma migrate deploy && prisma generate",
    "studio": "prisma studio",
    "tslint": "tslint --config ../tslint.json --fix --project .",
    "ngrok-http": "ngrok http 4000"
  },
  "prisma": {
    "schema": "./prisma/schema.prisma"
  },
  "keywords": ["GraphQL", "Apollo", "Express", "Prisma", "Docker"],
  "author": "Luis F. Domenech Ortiz",
  "license": "MIT",
  "devDependencies": {
    "@types/connect-redis": "^0.0.14",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.8",
    "@types/express-session": "^1.17.0",
    "@types/graphql": "^14.5.0",
    "@types/ioredis": "^4.17.5",
    "@types/node": "^14.11.8",
    "@types/node-schedule": "^1.3.1",
    "@types/nodemailer": "^6.4.0",
    "@types/pg": "^7.14.5",
    "@types/redis": "^2.8.28",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.4.7",
    "gen-env-types": "^1.0.4",
    "ngrok": "^4.2.2",
    "nodemon": "^2.0.5",
    "prisma": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@graphql-tools/schema": "^8.2.0",
    "@prisma/client": "^3.0.2",
    "@types/graphql-fields": "^1.3.4",
    "apollo-server-express": "^3.3.0",
    "argon2": "^0.28.2",
    "class-validator": "^0.12.2",
    "connect-redis": "^5.0.0",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "graphql": "^15.3.0",
    "graphql-fields": "^2.0.3",
    "graphql-jit": "^0.4.3",
    "graphql-scalars": "^1.10.1",
    "graphql-subscriptions": "^1.2.1",
    "graphql-type-json": "^0.3.2",
    "ioredis": "^4.17.3",
    "node-schedule": "^1.3.2",
    "nodemailer": "^6.4.14",
    "pg": "^8.4.1",
    "reflect-metadata": "^0.1.13",
    "tiny-lru": "^7.0.6",
    "type-graphql": "^1.1.0",
    "typegraphql-prisma": "^0.16.0",
    "uuid": "^8.3.1"
  }
}
